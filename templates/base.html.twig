<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        <!-- Google Font -->
        <link href="https://fonts.googleapis.com/css2?family=Cairo:wght@200;300;400;600;900&display=swap" rel="stylesheet">

        <!-- Css Styles -->
        <link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/font-awesome.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/elegant-icons.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/nice-select.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/jquery-ui.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/owl.carousel.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/slicknav.min.css') }}" type="text/css">
        <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}" type="text/css">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <!-- Page Preloder -->
        {# <div id="preloder">
            <div class="loader"></div>
        </div> #}

        <!-- Humberger Begin -->
        <div class="humberger__menu__overlay"></div>
        <div class="humberger__menu__wrapper">
            <div class="humberger__menu__logo">
                <a href="#"><img src="{{ asset('assets/img/logo.png') }}" alt=""></a>
            </div>
            <div class="humberger__menu__cart">
                <ul>
                    <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li>
                    <li><a href="#"><i class="fa fa-shopping-bag"></i> <span>3</span></a></li>
                </ul>
                <div class="header__cart__price">item: <span>$150.00</span></div>
            </div>
            <div class="humberger__menu__widget">
                <div class="header__top__right__language">
                    <img src="{{ asset('assets/img/language.png') }}" alt="">
                    <div>Français</div>
                    <span class="arrow_carrot-down"></span>
                    <ul>
                        <li><a href="#">Français</a></li>
                        <li><a href="#">Anglais</a></li>
                    </ul>
                </div>
                <div class="header__top__right__auth">
                    <a href="#"><i class="fa fa-user"></i> Inscription</a>
                </div>
            </div>
            <nav class="humberger__menu__nav mobile-menu">
                <ul>
                    <li class="active"><a href="{{ path('app_home') }}">Home</a></li>
                    <li><a href="./shop-grid.html">Shop</a></li>
                    <li><a href="#">Pages</a>
                        <ul class="header__menu__dropdown">
                            <li><a href="./shop-details.html">Shop Details</a></li>
                            <li><a href="./shoping-cart.html">Shoping Cart</a></li>
                            <li><a href="./checkout.html">Check Out</a></li>
                            <li><a href="./blog-details.html">Blog Details</a></li>
                        </ul>
                    </li>
                    <li><a href="./blog.html">Blog</a></li>
                    <li><a href="./contact.html">Contact</a></li>
                </ul>
            </nav>
            <div id="mobile-menu-wrap"></div>
            <div class="header__top__right__social">
                <a href="#"><i class="fa fa-facebook"></i></a>
                <a href="#"><i class="fa fa-whatsapp"></i></a>
                {# <a href="#"><i class="fa fa-twitter"></i></a> #}
                <a href="#"><i class="fa fa-linkedin"></i></a>
                {# <a href="#"><i class="fa fa-pinterest-p"></i></a> #}
            </div>
            <div class="humberger__menu__contact">
                <ul>
                    <li><i class="fa fa-envelope"></i> sav@nosprovisions.com</li>
                    <li>Free Shipping for all Order of $99</li>
                </ul>
            </div>
        </div>
        <!-- Humberger End -->

        <!-- Header Section Begin -->
        <header class="header">
            <div class="header__top">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 col-md-6">
                            <div class="header__top__left">
                                <ul>
                                    <li>
                                        <i class="fa fa-envelope"></i>
                                        <span style="color:green">
                                            <strong>sav@nosprovisions.com</strong>
                                        </span>
                                    </li>
                                    <li>
                                        <span style="color:red">
                                            <strong>Préparer mon départ</strong>
                                        </span>
                                    </li>
                                    <li>
                                        <span style="color:gold">
                                            <strong>Commander mes repas</strong>
                                        </span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6">
                            <div class="header__top__right">
                                <div class="header__top__right__social">
                                    <a href="#"><i class="fa fa-facebook"></i></a>
                                    <a href="#"><i class="fa fa-whatsapp"></i></a>
                                    {# <a href="#"><i class="fa fa-twitter"></i></a> #}
                                    <a href="#"><i class="fa fa-linkedin"></i></a>
                                    {# <a href="#"><i class="fa fa-pinterest-p"></i></a> #}
                                </div>
                                <div class="header__top__right__language">
                                    <img src="{{ asset('assets/img/france_flag.png') }}" alt="France Flag" width="30px" height="30px">
                                    {# <img src="{{ asset('assets/img/language.png') }}" alt=""> #}
                                    <div>Français</div>
                                    <span class="arrow_carrot-down"></span>
                                    <ul>
                                        <li><a href="#">Français</a></li>
                                        <li><a href="#">Anglais</a></li>
                                    </ul>
                                </div>
                                <div class="header__top__right__auth">
                                    {% if (app.request.pathInfo == path('app_login')) %}
                                        <a href="{{ path('app_register') }}"><i class="fa fa-user"></i> Inscription</a>
                                    {% elseif (app.user) %}
                                        <a href="{{ path('app_logout') }}"><i class="fa fa-user"></i> Déconnexion</a>
                                    {% else %}
                                        <a href="{{ path('app_login') }}"><i class="fa fa-user"></i> Connexion</a>
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-lg-3">
                        <div class="header__logo">
                            <a href="{{ path('app_home') }}"><img src="{{ asset('assets/img/logo7.png') }}" height="75px;" alt="logo"></a>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <nav class="header__menu">
                            <ul>
                                <li {% if (app.request.pathInfo == path('app_home')) %}class="active"{% endif %} {# class="active" #}>
                                    <a href="{{ path('app_home') }}">Accueil</a></li>
                                {# <li class="active"><a href="{{ path('app_home') }}">Ma Région</a></li> #}
                                <li {% if (app.request.pathInfo == path('extreme_north')) or (app.request.pathInfo == path('north')) or 
                                    (app.request.pathInfo == path('adamaoua')) or (app.request.pathInfo == path('center')) or 
                                    (app.request.pathInfo == path('north_west')) or (app.request.pathInfo == path('west')) or 
                                    (app.request.pathInfo == path('south_west')) or (app.request.pathInfo == path('littoral')) or 
                                    (app.request.pathInfo == path('south')) or (app.request.pathInfo == path('east')) %}class="active"{% endif %}>
                                    <a href="#">Ma Région</a>
                                    <ul class="header__menu__dropdown">
                                        <li><a href="{{ path('extreme_north') }}">Extrême Nord</a></li>
                                        <li><a href="{{ path('north') }}">Nord</a></li>
                                        <li><a href="{{ path('adamaoua') }}">Adamaoua</a></li>
                                        <li><a href="{{ path('center') }}">Centre</a></li>
                                        <li><a href="{{ path('north_west') }}">Nord-Ouest</a></li>
                                        <li><a href="{{ path('west') }}">Ouest</a></li>
                                        <li><a href="{{ path('south_west') }}">Sud Ouest</a></li>
                                        <li><a href="{{ path('littoral') }}">Littoral</a></li>
                                        <li><a href="{{ path('south') }}">Sud</a></li>
                                        <li><a href="{{ path('east') }}">Est</a></li>
                                    </ul>
                                </li>
                                {# <li><a href="{{ path('app_account') }}">Mon Espace</a></li> #}
                                <li {% if (app.request.pathInfo == path('app_products')) %}class="active"{% endif %}>
                                    <a href="{{ path('app_products') }}">Nos Produits</a>
                                </li>
                                {# {% if (app.user.roles[0] != 'ROLE_ADMIN') %}
                                    <li><a href="./contact.html">Contact</a></li>
                                {% endif %} #}
                                <li {% if (app.request.pathInfo == path('app_stats')) or (app.request.pathInfo == path('dia_bat')) or 
                                    (app.request.pathInfo == path('camembert'))%}class="active"{% endif %}>
                                    <a href="{{ path('app_stats') }}">Stats</a>
                                    <ul class="header__menu__dropdown">
                                        <li><a href="{{ path('dia_bat') }}">Commandes achetées</a></li>
                                        <li><a href="{{ path('camembert') }}">Pourcentage des commandes</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <div class="col-lg-3">
                        <div class="header__cart">
                            <ul>
                                {# <li><a href="#"><i class="fa fa-heart"></i> <span>1</span></a></li> #}
                                <li><a href="{{ path('app_cart') }}"><i class="fa fa-shopping-bag"></i> <span>{{ cart|length }}</span></a></li>
                                <li><a href="{{ path('app_account') }}"><i class="fa fa-user"></i></a></li>
                            </ul>
                            {# <div class="header__cart__price">item: <span>$150.00</span></div> #}
                            {# <div class="header__cart__price">total: <span>{{ (total / 100) | number_format(0, ',', '.') }} FCFA</span></div> #}
                            {# <div class="header__cart__price">Total: <span>{{ (total / 100) | number_format(0, ',', '.') }} FCFA</span></div> #}
                            <!-- Supposons que le montant total du panier soit déjà calculé dans une variable Twig appelée `total` -->
                            {# <div class="header__cart__price">Total: <span>{{ (total / 100) | number_format(0, ',', '.') }} FCFA</span></div> #}
                        </div>
                    </div>
                </div>
                <div class="humberger__open">
                    <i class="fa fa-bars"></i>
                </div>
            </div>
        </header>
        <!-- Header Section End -->

        {% block body %}{% endblock %}
        <!-- Footer Section Begin -->
        <footer class="footer spad">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-md-6 col-sm-6">
                        <div class="footer__about">
                            <div class="footer__about__logo">
                                <a href="{{ path('app_home') }}"><img src="{{ asset('assets/img/logo7.png') }}" alt=""></a>
                                {# <a href="{{ path('app_home') }}"><img src="{{ asset('assets/img/logo.png') }}" alt=""></a> #}
                            </div>
                            <ul>
                                <li>Address: 60-49 Road 11378 New York</li>
                                <li>Phone: +65 11.188.888</li>
                                <li>Email: sav@nosprovisions.com</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-6 offset-lg-1">
                        <div class="footer__widget">
                            <h6>Useful Links</h6>
                            <ul>
                                <li><a href="#">About Us</a></li>
                                <li><a href="#">About Our Shop</a></li>
                                <li><a href="#">Secure Shopping</a></li>
                                <li><a href="#">Delivery infomation</a></li>
                                <li><a href="#">Privacy Policy</a></li>
                                <li><a href="#">Our Sitemap</a></li>
                            </ul>
                            <ul>
                                <li><a href="#">Who We Are</a></li>
                                <li><a href="#">Our Services</a></li>
                                <li><a href="#">Projects</a></li>
                                <li><a href="#">Contact</a></li>
                                <li><a href="#">Innovation</a></li>
                                <li><a href="#">Testimonials</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-12">
                        <div class="footer__widget">
                            <h6>Join Our Newsletter Now</h6>
                            <p>Get E-mail updates about our latest shop and special offers.</p>
                            <form action="#">
                                <input type="text" placeholder="Enter your mail">
                                <button type="submit" class="site-btn">Subscribe</button>
                            </form>
                            <div class="footer__widget__social">
                                <a href="#"><i class="fa fa-facebook"></i></a>
                                <a href="#"><i class="fa fa-instagram"></i></a>
                                <a href="#"><i class="fa fa-whatsapp"></i></a>
                                {# <a href="#"><i class="fa fa-twitter"></i></a> #}
                                {# <a href="#"><i class="fa fa-pinterest"></i></a> #}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="footer__copyright">
                            <div class="footer__copyright__text"><p><!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
    Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. --></p></div>
                            <div class="footer__copyright__payment"><img src="{{ asset('assets/img/payment-item.png') }}" alt=""></div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- Footer Section End -->

        <!-- Js Plugins -->
        <script src="{{ asset('assets/js/jquery-3.3.1.min.js') }}"></script>
        <script src="{{ asset('assets/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('assets/js/jquery.nice-select.min.js') }}"></script>
        <script src="{{ asset('assets/js/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('assets/js/jquery.slicknav.js') }}"></script>
        <script src="{{ asset('assets/js/mixitup.min.js') }}"></script>
        <script src="{{ asset('assets/js/owl.carousel.min.js') }}"></script>
        <script src="{{ asset('assets/js/main.js') }}"></script>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <script src="{{ asset('assets/js/chart.js') }}"></script>
        {# <script src="https://kit.fontawesome.com/506909526a.js" crossorigin="anonymous"></script> #}
    </body>
</html>
